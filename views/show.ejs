<% include ./partials/header %>
    <% include ./partials/Navbar %>
        <div class="w-full min-h-screen px-10 py-10">
            <div class="cards flex flex-wrap gap-10">
                <% user.posts.forEach(function (elem) { %>
                    <div class="card w-40">
                        <div class=" w-full h-40 rounded-lg overflow-hidden">
                            <img src="/images/uploads/<%= elem.image %>" alt="" class="w-full h-full object-cover">
                        </div>
                        <h3 class="mt-2 text-md font-semibold">
                            <%=elem.title %>
                        </h3>
                    </div>
                    <% }) %>
            </div>
        </div>
        <% include ./partials/footer %>